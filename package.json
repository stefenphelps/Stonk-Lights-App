{
  "productName": "StonkLights",
  "main": "source/index.js",
  "scripts": {
    "package": "electron-forge package",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "make": "electron-forge make",
    "start": "electron-forge start"
  },
  "version": "1.0.0",
  "author": {
    "name": "Stefen Phelps",
    "email": "stefen@kelp.agency"
  },
  "license": "MIT",
  "description": "Setup and run your Stonklight!",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "AppIcon.icns"
      },
      "publishers": {
        "name": "@electron-forge/publisher-github",
        "config": {
          "repository": {
            "owner": "stefenphelps",
            "name": "Stonk-Lights-App"
          }
        }
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "StonkLights",
            "iconUrl": "https://www.kelp.agency/wp-content/uploads/2020/11/AppIcon.ico",
            "setupIcon": "icon/AppIcon.ico",
            "loadingGif": "icon/stonklight-loading.gif"
          }
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "name": "StonkLights",
            "format": "ULFO",
            "icon": "AppIcon.icns",
            "background": "AppBackground.png"
          },
          "platforms": [
            "darwin"
          ]
        }
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "electron": "^11.0.1"
  },
  "optionalDependencies": {},
  "keywords": [],
  "dependencies": {
    "@electron-forge/maker-dmg": "^6.0.0-beta.54",
    "axios": "^0.21.1",
    "electron-squirrel-startup": "^1.0.0",
    "jquery": "^3.5.1",
    "magic-home": "^2.6.1",
    "update-electron-app": "^2.0.1"
  },
  "name": "StonkLights"
}
